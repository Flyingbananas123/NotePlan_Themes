{
  "name": "Tomorrow Night",
  "style": "Dark",
  "author": {
    "name": "Brad Rokosz",
    "email": "hello@noteplan.co"
  },

  "editor": {
    "backgroundColor": "#1d1f21",
    "altBackgroundColor": "#1d1f21",
    "tintColor": "#8abeb7",
    "tintColor2": "#b294bb",
    "textColor": "#c5c8c6",
    "toolbarBackgroundColor": "#1d1f21",
    "toolbarIconColor": "#c5c8c6",
    "menuItemColor": "#c5c8c6",
    "shouldOverwriteFont": true
  },

  "styles": {
    "body": {
      "font": "AvenirNext-Regular",
      "size": 16,
      "color": "#c5c8c6"
    },

    "title1": {
      "color": "#b294bb",
      "size": 28,
      "font": "AvenirNext-DemiBold"
    },

    "title2": {
      "color": "#81a2be",
      "size": 24,
      "font": "AvenirNext-DemiBold"
    },

    "title3": {
      "color": "#f0c674",
      "size": 20,
      "font": "AvenirNext-DemiBold"
    },

    "title-mark1": {
      "color": "#f0c674",
      "size": 28,
      "font": "AvenirNext-DemiBold"
    },

    "title-mark2": {
      "color": "#8abeb7",
      "size": 24,
      "font": "AvenirNext-DemiBold"
    },

    "title-mark3": {
      "color": "#b294bb",
      "size": 20,
      "font": "AvenirNext-DemiBold"
    },

    "bold": {
      "color": "#de935f",
      "font": "AvenirNext-Bold"
    },

    "bold-left-mark": {
      "color": "#66de935f"
    },

    "bold-right-mark": {
      "color": "#66de935f"
    },

    "italic": {
      "color": "#b5bd68",
      "font": "AvenirNext-Italic"
    },

    "italic-left-mark": {
      "color": "#66b5bd68"
    },

    "italic-right-mark": {
      "color": "#66b5bd68"
    },

    "boldItalic": {
      "font": "AvenirNext-BoldItalic"
    },

    "boldItalic-left-mark": {
      "color": "#66CC6666"
    },

    "boldItalic-right-mark": {
      "color": "#66CC6666"
    },

    "code": {
      "font": "Menlo-Regular",
      "color": "#969896",
      "size": 15
    },

    "code-left-backtick": {
      "color": "#66969896"
    },

    "code-right-backtick": {
      "color": "#66969896"
    },

    "checked": {
      "headIndent": 33
    },

    "todo": {
      "font": "noteplanstate",
      "color": "#cc6666",
      "headIndent": 33,
      "size": 16
    },

    "checked-todo-characters": {
      "font": "noteplanstate",
      "headIndent": 33,
      "size": 16
    },

    "checked-completed-mark": {
      "regex": "(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[x\\] )",
      "matchPosition": 0,
      "color": "#b5bd68",
      "headIndent": 33
    },

    "checked-completed-content": {
      "regex": "(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[x\\] )(.*)",
      "matchPosition": 3,
      "color": "#AAc5c8c6",
      "headIndent": 33
    },

    "checked-scheduled": {
      "regex": "(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\>\\] )",
      "matchPosition": 0,
      "color": "#AAf0c674",
      "headIndent": 33
    },

    "checked-canceled": {
      "regex": "(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\-\\] )(.*)",
      "matchPosition": 0,
      "color": "#cc6666",
      "headIndent": 33
    },

    "special-char": {
      "font": "Menlo-Regular"
    },

    "tabbed": {
      "headIndent": 43
    },

    "quote-mark": {
      "color": "#b294bb",
      "font": "noteplanstate",
      "headIndent": 33,
      "size": 16
    },

    "quote-content": {
      "color": "#c5c8c6",
      "type": "italic",
      "headIndent": 33
    },

    "highlighted": {
      "regex": "(::)([^:]{1,})(::)",
      "matchPosition": 2,
      "isRevealOnCursorRange": true,
      "color": "1d1f21",
      "backgroundColor": "#de935f"
    },

    "highlighted-left-colon": {
      "regex": "(::)([^:]{1,})(::)",
      "matchPosition": 1,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true,
      "color": "#77de935f",
      "backgroundColor": "#1d1f21"
    },

    "highlighted-right-colon": {
      "regex": "(::)([^:]{1,})(::)",
      "matchPosition": 3,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true,
      "color": "#77de935f",
      "backgroundColor": "#1d1f21"
    },

    "flagged-1": {
      "regex": "(^|\\v)(\\h*(\\*|-)\\h*(\\[ \\]\\h+|\\h+)(\\!{1}\\h+.*))($|\\v)",
      "matchPosition": 5,
      "backgroundColor": "#22cc6666",
      "headIndent": 33
    },

    "flagged-2": {
      "regex": "(^|\\v)(\\h*(\\*|-)\\h*(\\[ \\]\\h+|\\h+)(\\!{2}\\h+.*))($|\\v)",
      "matchPosition": 5,
      "backgroundColor": "#55cc6666",
      "headIndent": 33
    },

    "flagged-3": {
      "regex": "(^|\\v)(\\h*(\\*|-)\\h*(\\[ \\]\\h+|\\h+)(\\!{3}\\h+.*))($|\\v)",
      "matchPosition": 5,
      "backgroundColor": "#AAcc6666",
      "headIndent": 33
    },

    "strikethrough": {
      "regex": "(~~)([^:]{1,})(~~)",
      "matchPosition": 2,
      "isRevealOnCursorRange": true,
      "color": "#AAc5c8c6",
      "strikethroughStyle": 1,
      "strikethroughColor": "#AAc5c8c6"
    },

    "strikethrough-left-tildes": {
      "regex": "(~~)([^:]{1,})(~~)",
      "matchPosition": 1,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true,
      "color": "#66c5c8c6"
    },

    "strikethrough-right-tildes": {
      "regex": "(~~)([^:]{1,})(~~)",
      "matchPosition": 3,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true,
      "color": "#66c5c8c6"
    },

    "link": {},

    "schedule-to-date-link": {},

    "done-date": {},

    "schedule-from-date-link": {},

    "note-title-link": {},

    "hashtag": {},

    "attag": {},

    "phonenumber": {}
  }
}
