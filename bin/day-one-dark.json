{
    "name": "Day One",
    "style": "Dark",
    "author": {
        "name": "Kennon Bickhart",
        "email": "kennon@bickh.art"
    },

    "editor": {
        "backgroundColor": "#1d1e25",
        "altBackgroundColor": "#1d1e25",
        "tintColor": "#44c0ff",
        "tintColor2": "#44c0ff",
        "textColor": "#ffffff",
        "toolbarBackgroundColor": "#3f3f3f",
        "toolbarIconColor": "#44c0ff",
        "menuItemColor": "#44c0ff",
        "shouldOverwriteFont": true
    },

    "styles": {

        "body": {
            "paragraphSpacing": 8,
            "paragraphSpacingBefore": 0,
            "color": "#ffffff",
            "font": ".AppleSystemUIFontRegular"
        },

        "title1": {
            "color": "#ffffff",
            "size": 30,
            "font": ".AppleSystemUIFontBold"
        },

        "title2": {
            "color": "#ffffff",
            "size": 24,
            "font": ".AppleSystemUIFontBold"
        },

        "title3": {
            "regex": "^\\h*(### )(.*)",
            "matchPosition": 2,
            "isRevealOnCursorRange": true,
            "color": "#ff6e6b",
            "size": 22,
            "font": ".AppleSystemUIFontBold"
        },

        "title4": {
            "regex": "^\\h*(#### )(.*)",
            "matchPosition": 2,
            "isRevealOnCursorRange": true,
            "color": "#7f7f7f",
            "size": 18,
            "font": ".AppleSystemUIFontBold"
        },

        "title5": {
            "regex": "^\\h*(##### )(.*)",
            "matchPosition": 2,
            "isRevealOnCursorRange": true,
            "color": "#ffffff",
            "size": 16,
            "underlineStyle": 3,
            "underlineColor": "#5f5f5f",
            "font": ".AppleSystemUIFontBoldItalic"
        },

        "title6": {
            "regex": "^\\h*(######+ )(.*)",
            "matchPosition": 2,
            "isRevealOnCursorRange": true,
            "backgroundColor": "#1c1c1c",
            "color": "#ffffff",
            "size": 16,
            "font": ".AppleSystemUIFontHeavy"
        },

        "title-mark1": {
            "color": "#50ffffff",
            "size": 16
        },

        "title-mark2": {
            "color": "#50ffffff",
            "size": 16
        },

        "title-mark3": {
            "regex": "^\\h*(### )",
            "matchPosition": 1,
            "isRevealOnCursorRange": true,
            "color": "#50ff6e6b",
            "size": 16
        },

        "title-mark4": {
            "regex": "^\\h*(#### )",
            "matchPosition": 1,
            "isRevealOnCursorRange": true,
            "isHiddenWithoutCursor": true,
            "isMarkdownCharacter": true,
            "color": "#70c2c3c4",
            "size": 16
        },

        "title-mark5": {
            "regex": "^\\h*(##### )",
            "matchPosition": 1,
            "isRevealOnCursorRange": true,
            "isHiddenWithoutCursor": true,
            "isMarkdownCharacter": true,
            "color": "#50ffffff",
            "size": 16,
            "paragraphSpacing": 0
        },

        "title-mark6": {
            "regex": "^\\h*(######+ )",
            "matchPosition": 1,
            "isRevealOnCursorRange": true,
            "isHiddenWithoutCursor": true,
            "isMarkdownCharacter": true,
            "color": "#50ffffff",
            "size": 16,
            "paragraphSpacing": 0
        },

        "bold": {
            "color": "#ffffff",
            "type": "bold"
        },

        "bold-left-mark": {
            "color": "#50ffffff"
        },

        "bold-right-mark": {
            "color": "#50ffffff"
        },

        "italic": {
            "color": "#ffffff"
        },

        "italic-left-mark": {
            "color": "#50ffffff"
        },

        "italic-right-mark": {
            "color": "#50ffffff"
        },

        "boldItalic": {
            "font": ".AppleSystemUIFontBoldItalic"
        },

        "boldItalic-left-mark": {
            "color": "#50ffffff"
        },

        "boldItalic-right-mark": {
            "color": "#50ffffff"
        },

        "code": {
            "font": "Menlo-Bold",
            "size": 16,
            "backgroundColor": "#333b40"
        },

        "code-left-backtick": {
            "color": "#50ffffff"
        },

        "code-right-backtick": {
            "color": "#50ffffff"
        },

        "checked": {
            "regex": "(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[x\\] )(.*)",
            "matchPosition": 0,
            "color": "#9ac476",
            "headIndent": 33
        },

        "checked-canceled": {
            "regex": "(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\-\\] )(.*)",
            "matchPosition": 0,
            "color": "#7f7f7f",
            "headIndent": 33,
            "strikethroughStyle": 1
        },

        "checked-scheduled": {
            "regex": "(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\>\\] )(.*)",
            "matchPosition": 0,
            "color": "#c27bd2",
            "headIndent": 33
        },

        "todo": {
            "font": "noteplanstate",
            "color": "#c2c3c4",
            "headIndent": 33,
            "size": 16
        },

        "checked-todo-characters": {
            "font": "noteplanstate",
            "headIndent": 33,
            "size": 16
        },

        "list-number": {
            "regex": "(^|\\v|\\t)([0-9]+\\.)",
            "matchPosition": 2,
            "color": "#cacacf"
        },

        "list-bullet": {
            "regex": "^((\\-|\\*)+)(?!\\s\\[(\\s|x|\\>|\\-)\\])",
            "matchPosition": 1,
            "color": "#44c0ff"
        },

        "special-char": {
            "font": "Menlo-Regular"
        },

        "tabbed": {
            "headIndent": 43
        },

        "quote-mark": {
            "color": "#515a5f",
            "font": "noteplanstate",
            "headIndent": 33,
            "size": 16
        },

        "quote-content": {
            "color": "#c7c7cc",
            "type": "italic",
            "headIndent": 33
        },

        "quote-citation": {
            "regex": "(~{1,})(\\s.+)",
            "matchPosition": 2,
            "font": ".AppleSystemUIFontBold"
        },

        "link": {},
        "schedule-to-date-link": {},
        "done-date": {},
        "schedule-from-date-link": {},
        "note-title-link": {},
        "hashtag": {},
        "attag": {},

        "highlighted": {
            "regex": "(::)([^:]{1,})(::)",
            "matchPosition": 2,
            "isRevealOnCursorRange": true,
            "backgroundColor": "#987614",
            "textColor": "#ffffff"
        },

        "highlighted-left-colon": {
            "regex": "(::)([^:]{1,})(::)",
            "matchPosition": 1,
            "isMarkdownCharacter": true,
            "isHiddenWithoutCursor": true,
            "isRevealOnCursorRange": true,
            "color": "#ffffff",
            "backgroundColor": "#987614"
        },

        "highlighted-right-colon": {
            "regex": "(::)([^:]{1,})(::)",
            "matchPosition": 3,
            "isMarkdownCharacter": true,
            "isHiddenWithoutCursor": true,
            "isRevealOnCursorRange": true,
            "color": "#ffffff",
            "backgroundColor": "#987614"
        },

        "strikethrough": {
            "regex": "(~~)([^~]{1,})(~~)",
            "matchPosition": 2,
            "isRevealOnCursorRange": true,
            "strikethroughStyle": 1
        },

        "strikethrough-left-mark": {
            "regex": "(~~)([^~]{1,})(~~)",
            "matchPosition": 1,
            "isMarkdownCharacter": true,
            "isHiddenWithoutCursor": true,
            "isRevealOnCursorRange": true
        },

        "strikethrough-right-mark": {
            "regex": "(~~)([^~]{1,})(~~)",
            "matchPosition": 3,
            "isMarkdownCharacter": true,
            "isHiddenWithoutCursor": true,
            "isRevealOnCursorRange": true
        },

        "flagged-1": {
            "regex": "(^|\\v)(\\h*(\\*|-)\\h*(\\[ \\]\\h+|\\h+)\\!{1}\\h+.*)($|\\v)",
            "matchPosition": 2,
            "isRevealOnCursorRange": true,
            "backgroundColor": "#40e74c3d",
            "headIndent": 33
        },

        "flagged-2": {
            "regex": "(^|\\v)(\\h*(\\*|-)\\h*(\\[ \\]\\h+|\\h+)\\!{2}\\h+.*)($|\\v)",
            "matchPosition": 2,
            "isRevealOnCursorRange": true,
            "backgroundColor": "#60e74c3d",
            "headIndent": 33
        },

        "flagged-3": {
            "regex": "(^|\\v)(\\h*(\\*|-)\\h*(\\[ \\]\\h+|\\h+)\\!{3}\\h+.*)($|\\v)",
            "matchPosition": 2,
            "isRevealOnCursorRange": true,
            "backgroundColor": "#aae74c3d",
            "headIndent": 33
        },

        "flagged-check-character": {
            "regex": "^((\\-|\\*)\\s\\[\\s\\]\\s)(?=!+)",
            "matchPosition": 1,
            "backgroundColor": "#1d1e25",
            "color": "#e74c3d",
            "headIndent": 33
        },

        "flagged-character": {
            "regex": "^((\\-|\\*)+)(\\s\\[\\s\\])(\\s\\!+\\s)",
            "matchPosition": 4,
            "isMarkdownCharacter": true,
            "isHiddenWithoutCursor": true,
            "isRevealOnCursorRange": true,
            "headIndent": 33
        }
    }
}
