        "code": {
            "font": "Menlo-Regular",
            "color": "#35bb54",
            "size": 15
        },
    
        "code-left-backtick": {
            "color": "#35bb54"
        },
        
        "code-right-backtick": {
            "color": "#35bb54"
        },
        
        "checked": {
            "color": "#547458",
            "headIndent": 33
        },
        
        "checked": {
            "regex": "(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[x\\] )(.*)",
            "matchPosition": 0,
            
            "color": "#547458",
            "headIndent": 33        
        },
        
        "checked-canceled": {
            "regex": "(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\-\\] )(.*)",
            "matchPosition": 0,
            
            "color": "#44FFCCC6",
            "headIndent": 33
        },
        
        "checked-scheduled": {
            "regex": "(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\>\\] )(.*)",
            "matchPosition": 0,
            
            "color": "#88CBCCFF",
            "headIndent": 33
        },
        
        "todo": {
            "font": "noteplanstate",
            "color": "#ff6ebf",
            "headIndent": 33,
            "size": 16
        },
        
        "checked-todo-characters": {
            "font": "noteplanstate",
            "headIndent": 33,
            "size": 16
        },
        
        "special-char": {
            "font": "Menlo-Regular",
        },
        
        "tabbed": {
            "headIndent": 43
        },
        
        "quote-mark": {
            "color": "#809580FF",
            "font": "noteplanstate",
            "headIndent": 33,
            "size": 16
        },
        
        "quote-content": {
            "color": "#80F7F7EF",
            "type": "italic",
            "headIndent": 33
        },
        
