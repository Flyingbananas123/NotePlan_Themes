        },
        
    "quote-content": {
        "color": "#fdf1c8",
        "font": "Noteworthy-Light",
        "headIndent": 33,	
        "size": 16,
        "lineSpacing":-1
        },
        
    "link": {                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       
          
        },
                        
    "schedule-to-date-link": {
       "font": "OpenSans-LightItalic"

        },
        
    "done-date": {
        "font": "OpenSans-LightItalic",
        "size": 16
        },
        
    
    "checked": {
          "regex": "(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[x\\] )(.*)",
          "matchPosition": 0,
          "color": "#94c1b1",
          "headIndent": 33,
          "lineSpacing":-1,
          "strikethroughStyle": 1
      
       },

    "checked-canceled": {
          "regex": "(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\-\\] )(.*)",
          "matchPosition": 0,
          "color": "#009bc9",
          "headIndent": 33,
          "lineSpacing":-1,
          "strikethroughStyle": 1
        },

    "checked-scheduled": {
          "regex": "(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\>\\] )(.*)",
          "matchPosition": 0,
          "color": "#ffcccc",
          "lineSpacing":-1,
          "headIndent": 33
       },

   
    "highlighted": {
        "regex": "(::)([^:]{1,})(::)",
         "matchPosition": 2,
         "isRevealOnCursorRange": true,
         "backgroundColor": "#69ddf6",
         "color": "#5C5C6A"
      },
 
    "highlighted-left-colon": {
        "regex": "(::)([^:]{1,})(::)",
        "matchPosition": 1,
        "isMarkdownCharacter": true,
        "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true,
      "color": "#fdf1c8"
      },
 
    "highlighted-right-colon": {
       "regex": "(::)([^:]{1,})(::)",
       "matchPosition": 3,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true,
      "color": "#fdf1c8"
      },

    "strikethrough": {
         "regex": "(~~)([^~]{1,})(~~)",
         "matchPosition": 2,
         "strikethroughStyle": 1,
         "color": "#009bc9",
         "isRevealOnCursorRange": true
        },
      
     "flagged-1": {
         "regex": "(^|\\v)(\\h*(\\- |\\* )\\!\\s.*)($|\\v)",
         "matchPosition": 2,
         "backgroundColor": "#ffcccc",
         "color": "#ff7f7f",
         "headIndent": 33
       },

    "flagged-2": {
        "regex": "(^|\\v)(\\h*(\\- |\\* )\\!\\!\\s.*)($|\\v)",
        "matchPosition": 2,
        "backgroundColor": "#ff6666",
        "color": "#ffffff",
        "headIndent": 33
      },

    "flagged-3": {
        "regex": "(^|\\v)(\\h*(\\- |\\* )\\!\\!\\!\\s.*)($|\\v)",
        "matchPosition": 2,
        "backgroundColor": "#cc0000",
        "color": "#ffffff",
       "headIndent": 33
      },
      
    "note-search-link":  {
        "regex": "//(\\S[^/]*\\S)//",
        "matchPosition": 1,
        "urlPosition": 1,
        "type": "link",
        "prefix": "noteplan://x-callback-url/search?text="
         },

    "schedule-from-date-link": {
         "font": "Noteworthy-Light",
         "lineSpacing":-4,
         "size": 14

        },
 
    "note-title-link": {
