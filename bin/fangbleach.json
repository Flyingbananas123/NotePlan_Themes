{
   "name":"Fangbleach",
   "style":"Light",
   "author":{
      "name":"MacBoeck",
      "email":"macboeck@icloud.com"
   },
   "editor":{
      "backgroundColor":"#DCD2CD",
      "altBackgroundColor":"#CFC6C1",
      "tintColor":"#443A36",
      "tintColor2":"#645B57",
      "textColor":"#251B17",
      "toolbarBackgroundColor":"#CFC6C1",
      "toolbarIconColor":"#413833",
      "menuItemColor":"#413833",
      "shouldOverwriteFont":false
   },
   "styles":{
      "body":{
         "font":"Palatino-Roman",
         "size":16,
         "color":"#251B17"
      },
      "title1":{
         "color":"#251B17",
         "size":28,
         "font":"Palatino-Bold"
      },
      "title2":{
         "color":"#443A36",
         "size":24,
         "font":"Palatino-Bold"
      },
      "title3":{
         "color":"#443A36",
         "size":20,
         "font":"Palatino-Bold"
      },
      "title-mark1":{
         "color":"#66251B17",
         "size":28,
         "font":"Palatino-Bold"
      },
      "title-mark2":{
         "color":"#66443A36",
         "size":24,
         "font":"Palatino-Bold"
      },
      "title-mark3":{
         "color":"#66443A36",
         "size":20,
         "font":"Palatino-Bold"
      },
      "bold":{
         "color":"#817873",
         "font":"Palatino-Bold"
      },
      "bold-left-mark":{
         "color":"#66817873"
      },
      "bold-right-mark":{
         "color":"#66817873"
      },
      "italic":{
         "color":"#463B37",
         "font":"Palatino-Italic"
      },
      "italic-left-mark":{
         "color":"#66463B37"
      },
      "italic-right-mark":{
         "color":"#66463B37"
      },
      "boldItalic":{
         "font":"Palatino-BoldItalic"
      },
      "boldItalic-left-mark":{
         "color":"#66817873"
      },
      "boldItalic-right-mark":{
         "color":"#66817873"
      },
      "code":{
         "font":"FiraCode-Regular",
         "color":"#584F49",
         "size":15
      },
      "code-left-backtick":{
         "color":"#66584F49"
      },
      "code-right-backtick":{
         "color":"#66584F49"
      },
      "checked":{
         "regex":"(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[x\\] )(.*)",
         "matchPosition":0,
         "color":"#88564C47",
         "headIndent":33
      },
      "checked-canceled":{
         "regex":"(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\-\\] )(.*)",
         "matchPosition":0,
         "color":"#44564C47",
         "headIndent":33,
         "strikethroughStyle":1
      },
      "checked-scheduled":{
         "regex":"(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\>\\] )(.*)",
         "matchPosition":0,
         "color":"#88564C47",
         "headIndent":33
      },
      "todo":{
         "font":"noteplanstate",
         "color":"#817873",
         "headIndent":33,
         "size":16
      },
      "checked-todo-characters":{
         "font":"noteplanstate",
         "headIndent":33,
         "size":16
      },
      "special-char":{
         "font":"Menlo-Regular"
      },
      "tabbed":{
         "headIndent":43
      },
      "quote-mark":{
         "color":"#564C47",
         "font":"noteplanstate",
         "headIndent":33,
         "size":16
      },
      "quote-content":{
         "color":"#413833",
         "type":"italic",
         "headIndent":33
      },
      "link":{
         
      },
      "schedule-to-date-link":{
         
      },
      "done-date":{
         
      },
      "schedule-from-date-link":{
         
      },
      "note-title-link":{
         
      },
      "hashtag":{
         
      },
      "attag":{
         
      },
      "phonenumber":{
         
      },
      "highlighted":{
         "regex":"(::)([^:]{1,})(::)",
         "matchPosition":2,
         "isRevealOnCursorRange":true,
         "backgroundColor":"#77786F6A"
      },
      "highlighted-left-colon":{
         "regex":"(::)([^:]{1,})(::)",
         "matchPosition":1,
         "isMarkdownCharacter":true,
         "isHiddenWithoutCursor":true,
         "isRevealOnCursorRange":true,
         "color":"#AA45A2E5",
         "backgroundColor":"#77786F6A"
      },
      "highlighted-right-colon":{
         "regex":"(::)([^:]{1,})(::)",
         "matchPosition":3,
         "isMarkdownCharacter":true,
         "isHiddenWithoutCursor":true,
         "isRevealOnCursorRange":true,
         "color":"#AA45A2E5",
         "backgroundColor":"#77786F6A"
      },
      "flagged-1":{
         "regex":"(^|\\v)(\\h*(\\*|-)\\h*(\\[ \\]\\h+|\\h+)\\!{1}\\h+.*)($|\\v)",
         "matchPosition":2,
         "backgroundColor":"#22C1B7B3",
         "headIndent":33
      },
      "flagged-2":{
         "regex":"(^|\\v)(\\h*(\\*|-)\\h*(\\[ \\]\\h+|\\h+)\\!{2}\\h+.*)($|\\v)",
         "matchPosition":2,
         "backgroundColor":"#55C1B7B3",
         "headIndent":33
      },
      "flagged-3":{
         "regex":"(^|\\v)(\\h*(\\*|-)\\h*(\\[ \\]\\h+|\\h+)\\!{3}\\h+.*)($|\\v)",
         "matchPosition":2,
         "backgroundColor":"#AAC1B7B3",
         "headIndent":33
      },
      "strikethrough":{
         "regex":"(^|[\\W_])(?:(?!\\1)|(?=^))((\\~|_)\\3)(?=\\S)(.*?\\S)(\\3\\3)(?!\\2)(?=[\\W_]|$)",
         "matchPosition":4,
         "strikethroughStyle":1,
         "isRevealOnCursorRange":true,
         "color":"#AACBCCC6",
         "strikethroughColor":"#39302D"
      },
      "strikethrough-left-tilde":{
         "regex":"(^|[\\W_])(?:(?!\\1)|(?=^))((\\~|_)\\3)(?=\\S)(.*?\\S)(\\3\\3)(?!\\2)(?=[\\W_]|$)",
         "matchPosition":2,
         "isMarkdownCharacter":true,
         "isHiddenWithoutCursor":true,
         "isRevealOnCursorRange":true,
         "color":"#4439302D"
      },
      "strikethrough-right-tilde":{
         "regex":"(^|[\\W_])(?:(?!\\1)|(?=^))(\\~|_)\\2(?=\\S)(.*?\\S)(\\2\\2)(?!\\2)(?=[\\W_]|$)",
         "matchPosition":4,
         "isMarkdownCharacter":true,
         "isHiddenWithoutCursor":true,
         "isRevealOnCursorRange":true,
         "color":"#4439302D"
      },
      "underline":{
         "regex":"(^|[\\W_])(?:(?!\\1)|(?=^))(\\~|_)(?=\\S)((?:(?!\\2).)*?\\S)(\\2)(?!\\2)(?=[\\W_]|$)",
         "matchPosition":3,
         "isRevealOnCursorRange":true,
         "underlineStyle":1,
         "underlineColor":"#372C28"
      },
      "underline-left-tilde":{
         "regex":"(^|[\\W_])(?:(?!\\1)|(?=^))(\\~|_)(?=\\S)((?:(?!\\2).)*?\\S)(\\2)(?!\\2)(?=[\\W_]|$)",
         "matchPosition":2,
         "isMarkdownCharacter":true,
         "isHiddenWithoutCursor":true,
         "isRevealOnCursorRange":true,
         "color":"#44372C28"
      },
      "underline-right-tilde":{
         "regex":"(^|[\\W_])(?:(?!\\1)|(?=^))(\\~|_)(?=\\S)((?:(?!\\2).)*?\\S)(\\2)(?!\\2)(?=[\\W_]|$)",
         "matchPosition":4,
         "isMarkdownCharacter":true,
         "isHiddenWithoutCursor":true,
         "isRevealOnCursorRange":true,
         "color":"#44372C28"
      }
   }
}