{
  "name": "Dark Purple",
  "style": "Dark",
  "author": {
    "name": "Jane",
    "email": "jane@email.com"
  },
  "editor": {
    "backgroundColor": "#313638",
    "altBackgroundColor": "#525152",
    "tintColor": "#D6B80A",
    "tintColor2": "#729781",
    "textColor": "#D7D6D7",
    "toolbarBackgroundColor": "#313638",
    "toolbarIconColor": "#FAFAFA",
    "menuItemColor": "#FAFAFA",
    "shouldOverwriteFont": true
  },
  "styles": {
    "body": {
      "font": "AvenirNext-Regular",
      "size": 16,
      "color": "#D7D6D7"
    },
    "title1": {
      "color": "#B47E9A",
      "size": 28,
      "font": "AvenirNext-DemiBold"
    },
    "title2": {
      "color": "#C398AE",
      "size": 24,
      "font": "AvenirNext-DemiBold"
    },
    "title3": {
      "color": "#CAA5B8",
      "size": 20,
      "font": "AvenirNext-DemiBold"
    },
    "title-mark1": {
      "color": "#736372",
      "size": 28,
      "font": "AvenirNext-DemiBold"
    },
    "title-mark2": {
      "color": "#8D7C8C",
      "size": 24,
      "font": "AvenirNext-DemiBold"
    },
    "title-mark3": {
      "color": "#A0929F",
      "size": 20,
      "font": "AvenirNext-DemiBold"
    },
    "bold": {
      "color": "#62929E",
      "font": "AvenirNext-Bold"
    },
    "bold-left-mark": {
      "color": "#62929E"
    },
    "bold-right-mark": {
      "color": "#62929E"
    },
    "italic": {
      "color": "#62929E",
      "font": "AvenirNext-Italic"
    },
    "italic-left-mark": {
      "color": "#62929E"
    },
    "italic-right-mark": {
      "color": "#62929E"
    },
    "boldItalic": {
      "font": "AvenirNext-BoldItalic"
    },
    "boldItalic-left-mark": {
      "color": "#62929E"
    },
    "boldItalic-right-mark": {
      "color": "#62929E"
    },
    "code": {
      "font": "Menlo-Regular",
      "color": "#D97430",
      "size": 15
    },
    "code-left-backtick": {
      "color": "#D97430"
    },
    "code-right-backtick": {
      "color": "#D97430"
    },
    "checked": {
      "headIndent": 33
    },
    "todo": {
      "font": "noteplanstate",
      "color": "#72AC99",
      "headIndent": 33,
      "size": 16
    },
    "checked-todo-characters": {
      "font": "noteplanstate",
      "headIndent": 33,
      "size": 16
    },
    "checked-completed-mark": {
      "regex": "(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[x\\] )",
      "matchPosition": 0,
      "color": "#98C3B4",
      "headIndent": 33
    },
    "checked-completed-content": {
      "regex": "(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[x\\] )(.*)",
      "matchPosition": 3,
      "color": "#98C3B4",
      "headIndent": 33
    },
    "checked-scheduled": {
      "regex": "(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\>\\] )",
      "matchPosition": 0,
      "color": "#72AC99",
      "headIndent": 33
    },
    "checked-canceled": {
      "regex": "(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\-\\] )(.*)",
      "matchPosition": 0,
      "color": "#538D7A",
      "headIndent": 33
    },
    "special-char": {
      "font": "Menlo-Regular"
    },
    "tabbed": {
      "headIndent": 43
    },
    "quote-mark": {
      "color": "#72AC99",
      "font": "noteplanstate",
      "headIndent": 33,
      "size": 16
    },
    "quote-content": {
      "color": "#C2C1C2",
      "type": "italic",
      "headIndent": 33
    },
    "highlighted": {
      "regex": "(::)([^:]{1,})(::)",
      "matchPosition": 2,
      "isRevealOnCursorRange": true,
      "backgroundColor": "#AAEECEBF"
    },
    "highlighted-left-colon": {
      "regex": "(::)([^:]{1,})(::)",
      "matchPosition": 1,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true,
      "color": "#e1c4dee5",
      "backgroundColor": "#77E5B49E"
    },
    "highlighted-right-colon": {
      "regex": "(::)([^:]{1,})(::)",
      "matchPosition": 3,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true,
      "color": "#e1c4dee5",
      "backgroundColor": "#77E5B49E"
    },
    "flagged-1": {
      "regex": "(^|\\v)(\\h*(\\*|-)\\h*(\\[ \\]\\h+|\\h+)(\\!{1}\\h+.*))($|\\v)",
      "matchPosition": 5,
      "backgroundColor": "#22D0021B",
      "headIndent": 33
    },
    "flagged-2": {
      "regex": "(^|\\v)(\\h*(\\*|-)\\h*(\\[ \\]\\h+|\\h+)(\\!{2}\\h+.*))($|\\v)",
      "matchPosition": 5,
      "backgroundColor": "#55D0021B",
      "headIndent": 33
    },
    "flagged-3": {
      "regex": "(^|\\v)(\\h*(\\*|-)\\h*(\\[ \\]\\h+|\\h+)(\\!{3}\\h+.*))($|\\v)",
      "matchPosition": 5,
      "backgroundColor": "#AAD0021B",
      "headIndent": 33
    },
    "strikethrough": {
      "regex": "(~~)([^:]{1,})(~~)",
      "matchPosition": 2,
      "isRevealOnCursorRange": true,
      "color": "#D7D6D7",
      "strikethroughStyle": 1,
      "strikethroughColor": "#B3B6BD"
    },
    "strikethrough-left-tildes": {
      "regex": "(~~)([^:]{1,})(~~)",
      "matchPosition": 1,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true,
      "color": "#66c5c5c0"
    },
    "strikethrough-right-tildes": {
      "regex": "(~~)([^:]{1,})(~~)",
      "matchPosition": 3,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true,
      "color": "#66c5c5c0"
    },
    "link": {},
    "schedule-to-date-link": {},
    "done-date": {},
    "schedule-from-date-link": {},
    "note-title-link": {},
    "hashtag": {},
    "attag": {},
    "phonenumber": {}
  }
}